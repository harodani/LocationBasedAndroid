package dataStore;

import android.text.format.Time;

public class RunningData extends Data {		
	private Time time;
	
	public RunningData() {
	}
	
	public RunningData(Time time){
		this.time = time;			
	}
	
	public Time getTime(){			
		return this.time;
	}
	
	public void setTime(Time time){
		this.time = time;
	}
}
