package my.location.locationPolicy;

import java.util.List;

public class LocationPolicy {	
	private int policyId;	
	private String policyName;	
	private DefinedLocation definedLocation;	
	private List<DefinedAction> actions;
	
	public int getPolicyId() {
		return policyId;
	}
	
	public void setPolicyId(int policyId) {
		this.policyId = policyId;
	} 

	public String getPolicyName() {
		return policyName;
	}

	public void setPolicyName(String policyName) {
		this.policyName = policyName;
	}

	public DefinedLocation getDefinedLocation() {
		return definedLocation;
	}

	public void setDefinedLocation(DefinedLocation definedLocation) {
		this.definedLocation = definedLocation;
	}

	public List<DefinedAction> getActions() {
		return actions;
	}

	public void setActions(List<DefinedAction> actions) {
		this.actions = actions;
	}
}